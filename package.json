{
  "name": "vasiti-test-backend",
  "version": "1.0.0",
  "description": "vasiti test backend-api",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon index.js --exec babel-node",
