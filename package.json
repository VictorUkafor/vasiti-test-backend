  "dependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "^7.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "airbnb-browser-shims": "^3.3.0",
    "babel-preset-airbnb": "^4.4.0",
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
